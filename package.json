{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun services:up && bun infra/scripts/waitPostgres.ts && bun prisma:migrate:dev && next dev -p 3005",
    "build": "echo 'Building for production ⚠️' && bunx prisma generate && bunx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "test": "dotenv -e .env.test -- bun run services:up && dotenv -e .env.test -- bun infra/scripts/waitPostgres.ts && dotenv -e .env.test -- bun infra/scripts/createDatabase.ts && dotenv -e .env.test -- bunx prisma migrate dev && dotenv -e .env.test -- bun test tests --verbose --bail",
    "test:watch": "bun test --watch",
    "services:up": "docker compose --env-file .env -f infra/dev-compose.yaml up -d",
    "services:stop": "docker compose --env-file .env -f infra/dev-compose.yaml stop",
    "services:down": "docker compose --env-file .env -f infra/dev-compose.yaml down",
    "prisma:generate": "bunx prisma generate",
    "prisma:migrate:dev": "bunx prisma migrate dev",
    "postinstall": "bunx prisma generate",
    "prepare": "husky"
  },
  "prisma": {
    "schema": "infra/prisma/schema.prisma"
  },
